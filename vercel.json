{
    "version": 2,
    "builds": [
        {
            "src": "fortune/*.py",
            "use": "@vercel/python"
        },
        {
            "src": "fortune/templates/*",
            "use": "@vercel/static"
        },
        {
            "src": "fortune/static/*/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/fortune/templates/index.html"
        },
        {
            "src": "/generate",
            "dest": "/fortune/__init__.py"
        }
    ]
}